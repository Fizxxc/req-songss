{
  "version": 2,
  "builds": [
    {
      "src": "api/request.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/token.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/request",
      "dest": "/api/request.js"
    },
    {
      "src": "/api/token",
      "dest": "/api/token.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
